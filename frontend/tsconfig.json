{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"module": "preserve",
		"moduleResolution": "bundler",
		"target": "esnext",
		"jsx": "react-jsx",
		"lib": ["ESNext", "DOM"],
		"types": ["vite/client"],
		"skipLibCheck": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true
	},
	"include": ["src", "vite.config.ts"]
}
